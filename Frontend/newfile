<script id="toggle-switches">
document.addEventListener('DOMContentLoaded', function() {
const toggles = document.querySelectorAll('input[type="checkbox"]');
toggles.forEach(toggle => {
toggle.addEventListener('change', function() {
const card = this.closest('.bg-white');
const statusBadge = card.querySelector('.px-2.py-1');
if (this.checked) {
statusBadge.className = 'px-2 py-1 bg-green-100 text-green-800 rounded-full text-xs font-medium';
statusBadge.textContent = '활성';
} else {
statusBadge.className = 'px-2 py-1 bg-gray-100 text-gray-800 rounded-full text-xs font-medium';
statusBadge.textContent = '비활성';
}
updateSystemStats();
});
});
});
</script>
<script id="system-stats">
function updateSystemStats() {
const activeDecks = document.querySelectorAll('input[type="checkbox"]:checked').length;
const activeDeckElement = document.querySelector('.text-2xl.font-bold.text-primary');
if (activeDeckElement) {
activeDeckElement.textContent = activeDecks;
}
}
</script>
<script id="save-functionality">
document.addEventListener('DOMContentLoaded', function() {
const saveButton = document.querySelector('button:contains("변경사항 저장")') ||
document.querySelector('header button');
const toast = document.getElementById('toast');
if (saveButton) {
saveButton.addEventListener('click', function() {
this.disabled = true;
this.textContent = '저장 중...';
setTimeout(() => {
this.disabled = false;
this.textContent = '변경사항 저장';
toast.classList.remove('translate-x-full');
setTimeout(() => {
toast.classList.add('translate-x-full');
}, 3000);
}, 1500);
});
}
});
</script>
</body>
</html>


